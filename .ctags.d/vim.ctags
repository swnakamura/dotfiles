--langdef=Vimhelp
--langmap=Vimhelp:.txt
--regex-Vimhelp=/\|([[:graph:]]+)\|/\1/r,refs/
--regex-Vimhelp=/\*([[:graph:]]+)\*$/\1/l,labels/
--mline-regex-Vimhelp=/^=====+[[:space:]]*([[:print:]]+)/\1/s,sections/{mgroup=1}
--mline-regex-Vimhelp=/^-----+[[:space:]]*([[:print:]]+)/\1/s,subsections/{mgroup=1}
